# baseURI: https://www.sodalite.eu/ontologies/exchange/

@prefix :         <https://www.sodalite.eu/ontologies/exchange/> .
@prefix exchange: <https://www.sodalite.eu/ontologies/exchange#> .
@prefix owl:      <http://www.w3.org/2002/07/owl#> .
@prefix rdf:      <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:     <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd:      <http://www.w3.org/2001/XMLSchema#> .

:
    rdf:type        owl:Ontology ;
    owl:versionInfo "Created with TopBraid Composer" ;
.

:AADM
    rdf:type        owl:Class ;
    rdfs:subClassOf owl:Thing ;
.

:Attribute
    rdf:type        owl:Class ;
    rdfs:subClassOf :Parameter ;
.

:Capability
    rdf:type        owl:Class ;
    rdfs:subClassOf :Entity ;
.

:Entity
    rdf:type        owl:Class ;
    rdfs:subClassOf :Resource ;
.

:Interface
    rdf:type        owl:Class ;
    rdfs:subClassOf :Entity ;
.

:Node
    rdf:type        owl:Class ;
    rdfs:subClassOf :Entity ;
.

:Parameter
    rdf:type        owl:Class ;
    rdfs:subClassOf :Resource ;
.

:Property
    rdf:type        owl:Class ;
    rdfs:subClassOf :Parameter ;
.

:Relationship
    rdf:type        owl:Class ;
    rdfs:subClassOf :Entity ;
.

:Requirement
    rdf:type        owl:Class ;
    rdfs:subClassOf :Entity ;
.

:Resource
    rdf:type        owl:Class ;
    rdfs:subClassOf owl:Thing ;
.

:Template
    rdf:type        owl:Class ;
    rdfs:subClassOf :Entity ;
.

:attributes
    rdf:type    owl:ObjectProperty ;
    rdfs:domain :Entity ;
    rdfs:range  :Attribute ;
.

:capabilities
    rdf:type    owl:ObjectProperty ;
    rdfs:domain :Entity ;
    rdfs:range  :Capability ;
.

:derivesFrom
    rdf:type    owl:DatatypeProperty ;
    rdfs:domain :Entity ;
    rdfs:range  xsd:string ;
.

:description
    rdf:type   owl:DatatypeProperty ;
    rdfs:range xsd:string ;
.

:hasParameter
    rdf:type    owl:ObjectProperty ;
    rdfs:domain :Resource ;
    rdfs:range  :Parameter ;
.

:interfaces
    rdf:type    owl:ObjectProperty ;
    rdfs:domain :Entity ;
    rdfs:range  :Interface ;
.

:name
    rdf:type    owl:DatatypeProperty ;
    rdfs:domain :Resource ;
    rdfs:range  xsd:string ;
.

:properties
    rdf:type    owl:ObjectProperty ;
    rdfs:domain :Entity ;
    rdfs:range  :Property ;
.

:requirements
    rdf:type    owl:ObjectProperty ;
    rdfs:domain :Entity ;
    rdfs:range  :Requirement ;
.

:type
    rdf:type    owl:DatatypeProperty ;
    rdfs:domain :Template ;
    rdfs:range  xsd:string ;
.

:userId
    rdf:type    owl:DatatypeProperty ;
    rdfs:domain :AADM ;
    rdfs:range  xsd:string ;
.

:value
    rdf:type    rdf:Property ;
    rdfs:domain :Parameter ;
.
